<%if(messages && messages.info){%>
<div class="alert alert-success alert-dismissible" id="errorInfo" style="position: absolute; top: 1em; right: 1em; z-index: 1000; width: 30%;">
    <button type="button" class="close" data-dismiss="alert" id="closeBtn">&times;</button>
    <%=messages.info%>
</div>
<%} else {%>   
<div></div>
<%}%>